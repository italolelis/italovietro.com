// https://github.com/microsoft/vscode-dev-containers/tree/v0.128.0/containers/hugo
{
	"name": "Hugo",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "1.19-bullseye",
			"VERSION": "latest"
		}
	},
	"extensions": [
		"bungcip.better-toml",
		"davidanson.vscode-markdownlint"
	],
	"containerEnv": {
		"HUGO_SERVICES_INSTAGRAM_ACCESSTOKEN": "xxx"
	},
	"forwardPorts": [1313],
	"remoteUser": "vscode"
}
